@model ZazaInc.Models.Solicitante

@{
    ViewBag.Title = "Details";
}

<link href="~/CSS/Terminacion.css" rel="stylesheet" />
<p id="p">Mostrando detalles...</p>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombre_Solicitante)
        </dd>

        <dt>
            Apellido
        </dt>

        <dd>
            @Html.DisplayFor(model => model.apellido_Solicitante)
        </dd>

        <dt>
            Cedula
        </dt>

        <dd>
            @Html.DisplayFor(model => model.cedula_Solicitante)
        </dd>

        <dt>
            Correo Electronico
        </dt>

        <dd>
            @Html.DisplayFor(model => model.correo_Solicitante)
        </dd>

    </dl>
</div>
<button id="botonRegistro" class="btn btn-primary">
    @Html.ActionLink("Volver al listado", "Index", new { @class = "addy" }, new { @class = "addy" })
</button>

<button id="botonRegistro" class="btn btn-primary" style="background-color:#219fd6">
    @Html.ActionLink("Editar", "Edit", new { id = Model.id_Solicitante }, new { @class = "addy" })
</button>
