@model IEnumerable<ZazaInc.Models.Empleado>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<li>@Html.ActionLink("Actualizar informacion", "Index", "Empleadoes")</li>
<li>@Html.ActionLink("Gestionar Vacaciones", "Index", "Vacaciones")</li>
<p>
    @Html.ActionLink("Crear registro", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nombre_empleado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.apellido_empleado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.posicion_empleado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cedula_empleado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.correo_usuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.pago_empleado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.horario_empleado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.departamento)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.nombre_empleado)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.apellido_empleado)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.posicion_empleado)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.cedula_empleado)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.correo_usuario)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.pago_empleado)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.horario_empleado)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.departamento)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.id_empleado }) |
        @Html.ActionLink("Details", "Details", new { id = item.id_empleado }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.id_empleado })
    </td>
</tr>
}

</table>
