@model IEnumerable<ZazaInc.Models.Terminar>

@{
    ViewBag.Title = "Index";
}

<h2>Terminacion laboral</h2>

<p>
    @Html.ActionLink("Añadir registro", "Create")
</p>
<table class="table">
    <tr>

        <th>
            Nombre del empleado
        </th>
        <th>
            Nombre del usuario responsable
        </th>
        <th>
            @Html.DisplayNameFor(model => model.razon)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>

    <td>
        @Html.DisplayFor(modelItem => item.Empleado.nombre_empleado)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Usuario.nombre_usuario)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.razon)
    </td>
    <td>
        <!--@Html.ActionLink("Edit", "Edit", new { id = item.ID_Terminar }) |-->
        @Html.ActionLink("Detalles", "Details", new { id = item.ID_Terminar }) |
        <!-- @Html.ActionLink("Borrar", "Delete", new { id = item.ID_Terminar })-->
    </td>
</tr>
}

</table>
